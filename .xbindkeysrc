############################
# Beginning of xbindkeysrc #
############################

# Brightness Up
"xbacklight -5"
    m:0x0 + c:232
# Brightness Down
"xbacklight +5"
    m:0x0 + c:233
# Toggle volume
"/usr/bin/pulseaudio-ctl mute"
    m:0x0 + c:121

# Toggle mic
"/usr/bin/pulseaudio-ctl mute-input"
    m:0x0 + c:198
# Volume up
"/usr/bin/pulseaudio-ctl up 5"
    m:0x0 + c:123
# Volume down
"/usr/bin/pulseaudio-ctl down 5"
    m:0x0 + c:122
# Screenshot Full Screen (Mod4 + Print)
"import -window root /home/cyber/Pictures/screenshot-`date +%m%d%Y%H%M%S`.png"
   m:0x40 + c:107
    
# Select and Screenshot  Window (Print)
"import -frame /home/cyber/Pictures/screenshot-`date +%m%d%Y%H%M%S`.png"
    m:0x0 + c:107


############################
#    End of xbindkeysrc    #
############################


